{
    "name": "shengyou/sibala-team1",
    "description": "91dojo sibala project team 1",
    "keywords": ["php", "phpunit", "91dojo", "sibala"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
